        <source>Show the details of calendar ID %s</source>
        <target>نمایش جزئیات سالنمای شناسه %s</target>
      </trans-unit>
      <trans-unit id="tl_calendar.edit">
        <source>Edit calendar ID %s</source>
        <target>ویرایش سالنمای شناسه %s</target>
      </trans-unit>
      <trans-unit id="tl_calendar.editmeta">
        <source>Edit the calendar settings</source>
        <target>ویرایش تنظیمات تقویم</target>
      </trans-unit>
      <trans-unit id="tl_calendar.editheader">
        <source>Edit the settings of calendar ID %s</source>
        <target>ویرایش تنظیمات سالنمای شناسه %s</target>
      </trans-unit>
      <trans-unit id="tl_calendar.copy">
        <source>Duplicate calendar ID %s</source>
        <target>تکثیر سالنمای شناسه %s</target>
      </trans-unit>
      <trans-unit id="tl_calendar.delete">
        <source>Delete calendar ID %s</source>
        <target>حذف سالنمای شناسه %s</target>
      </trans-unit>
