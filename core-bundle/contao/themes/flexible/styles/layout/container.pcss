/* Container */
#container {
    display: flex;
    min-height: calc(100vh - 40px);
    margin: 0 auto;
    max-width: var(--backend-width, 100%);
}

#container turbo-frame {
    display: contents;
}

.popup #container {
    padding: 0;
    width: auto;
    min-height: 0;
    max-width: none;
    display: block;
}
