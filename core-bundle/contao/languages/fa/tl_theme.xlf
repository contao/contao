        <source>Edit the theme settings</source>
        <target>ویرایش تنظیمات پوسته</target>
      </trans-unit>
      <trans-unit id="tl_theme.new.0">
        <source>New</source>
        <target>جدید</target>
      </trans-unit>
      <trans-unit id="tl_theme.new.1">
        <source>Create a new theme</source>
        <target>ایجاد یک پوسته جدید</target>
      </trans-unit>
