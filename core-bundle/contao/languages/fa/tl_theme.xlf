        <source>Edit the page layouts of theme ID %s</source>
        <target>ویرایش چیدمان برگه‌ها پوسته شناسه %s</target>
      </trans-unit>
      <trans-unit id="tl_theme.imageSizes">
        <source>Edit the image sizes of theme ID %s</source>
        <target>ویرایش اندازه نصویر پوسته شناسه %s</target>
      </trans-unit>
      <trans-unit id="tl_theme.exportTheme">
        <source>Export theme ID %s</source>
        <target>خارج کردن پوسته شناسه %s</target>
      </trans-unit>
