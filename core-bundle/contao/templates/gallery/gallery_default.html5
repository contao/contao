
<ul class="cols_<?= $this->perRow ?>">
  <?php foreach ($this->body as $row): ?>
    <?php foreach ($row as $col): ?>
      <?php if ($col->addImage): ?>
        <li><?php $this->insert('image', (array) $col); ?></li>
      <?php endif; ?>
    <?php endforeach; ?>
  <?php endforeach; ?>
</ul>
