
{% if title|default %}
  <h1>{{ title }}</h1>
{% endif %}

{% if wildcard|default %}
  <div class="tl_gray">
    <span class="upper">{{ wildcard }}</span>
    {% if link|default %}
      <br>
      {{ link }} (<a href="{{ href ~ '&rt=' ~ contao.request_token }}" class="tl_gray">ID: {{ id }}</a>)
    {% endif %}
  </div>
{% endif %}
