        <source>Title</source>
        <target>Başlık</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.title.1">
        <source>Please enter a news archive title.</source>
        <target>Lütfen haber arşivine başlık girin</target>
      </trans-unit>
