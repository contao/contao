{
    "name": "contao/news-bundle",
    "type": "contao-bundle",
    "description": "Contao 4 news bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Leo Feyer",
            "homepage": "https://github.com/leofeyer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/framework-bundle": "~2.8|~3.0",
        "contao/core-bundle": "dev-release/4.3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~1.8",
        "phpunit/phpunit": "~4.5",
        "satooshi/php-coveralls": "~0.6",
        "symfony/phpunit-bridge": "~2.8|~3.0"
    },
    "conflict": {
        "contao/core": "*"
    },
    "autoload": {
        "psr-4": {
            "Contao\\NewsBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config",
            "src/Resources/contao/dca",
            "src/Resources/contao/languages",
            "src/Resources/contao/templates"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Contao\\NewsBundle\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-release/4.3.0": "4.3.x-dev",
            "dev-develop": "4.4.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
