        "symfony/asset": "^5.4",
        "symfony/cache": "^5.4",
        "symfony/config": "^5.4",
        "symfony/console": "^5.4",
        "symfony/debug-bundle": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/deprecation-contracts": "^2.1 || ^3.0",
